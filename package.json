{
  "engines": {
    "composer": "^0.14.2"
  },
  "name": "my-network",
  "version": "0.0.1",
  "description": "My very first Hyperledger Composer Network",
  "scripts": {
    "prepublish": "mkdirp ./dist && composer archive create  --sourceType dir --sourceName . -a ./dist/my-network.bna",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.json",
    "test": "mocha -t 5000 --recursive",
    "quicktest": "mocha -t 5000 --recursive --full-trace -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer-sample-networks.git"
  },
  "keywords": [
    "composer",
    "composer-network"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "devDependencies": {
    "browserfs": "latest",
    "chai": "latest",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "composer-admin": "latest",
    "composer-cli": "latest",
    "composer-client": "latest",
    "composer-connector-embedded": "latest",
    "eslint": "^4.4.1",
    "istanbul": "latest",
    "jsdoc": "latest",
    "license-check": "latest",
    "mkdirp": "latest",
    "mocha": "latest",
    "moment": "latest",
    "store": "^2.0.12"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*",
      "!./scripts/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  }
}
